														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="250"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="GorodN">
														<xs:annotation>
															<xs:documentation>Город</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="250"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="NPunktN">
