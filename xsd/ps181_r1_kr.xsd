														</xs:simpleType>
													</xs:element>
													<xs:element name="UlicaN">
														<xs:annotation>
															<xs:documentation>Улица (проспект, переулок, и т.д.)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="250"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DomN">
														<xs:annotation>
															<xs:documentation>Номер дома (владение)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="KorpusN">
														<xs:annotation>
															<xs:documentation>Корпус (строение)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="OfisN">
														<xs:annotation>
															<xs:documentation>Офис (квартира)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="RegNumN">
														<xs:annotation>
															<xs:documentation>
																Основной государственный регистрационный номер (13 или 15 знаков) или NN/NNNN
															</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="[0-9]{13}|[0-9]{15}|([0-9]{2}/[0-9]{1,12})"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="RegDateN" type="DateStringType">
														<xs:annotation>
															<xs:documentation>
																Дата внесения записи в государственный реестр
															</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="InnN">
														<xs:annotation>
															<xs:documentation>Код ИНН/КПП</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="(([0-9]{10})|([0-9]{12}))(()|(/))(()|([0-9]{9}))"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DocNum">
														<xs:annotation>
															<xs:documentation>
																Номер документа (или БН), подтверждающего уступку требования или перевод долга по кредитному договору
															</xs:documentation>
														</xs:annotation>
