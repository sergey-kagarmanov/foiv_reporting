																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DocDate" type="DateStringType">
														<xs:annotation>
															<xs:documentation>
																Дата документа, подтверждающего уступку требования или перевод долга по кредитному договору [дд/мм/гггг]
															</xs:documentation>
														</xs:annotation>
